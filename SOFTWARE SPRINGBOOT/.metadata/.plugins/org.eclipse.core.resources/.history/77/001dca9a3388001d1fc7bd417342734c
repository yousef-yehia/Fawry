package com.example.demo.Model.DiscountsModel;

public abstract class Discount {
	String type;
	double value;
	
	public Discount(String type, double value) {
		this.type = type;
		this.value = value;
	}
	public String getType() {
		return type;
	}
	public double getValue() {
		return value;
	}
	public void setValue(double value) {
		this.value = value;
	}

}
