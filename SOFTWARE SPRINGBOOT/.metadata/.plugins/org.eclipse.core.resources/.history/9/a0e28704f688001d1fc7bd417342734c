package com.example.demo.Model.DiscountsModel;

public abstract class Discount {
	String type;
	double value;
	String service = "";
	public Discount(double value) {
		this.value = value;
	}
	public String getType() {
		return type;
	}
	public double getValue() {
		return value;
	}
	public String getService() {
		return service;
	}
}
