package com.example.demo.Model.Transactions;

public abstract class Transaction {
	double amount;
	int userID;
	String type;
	public double getAmount() {
		return amount;
	}
	public int getUserID() {
		return userID;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
}
