package com.example.demo.Model;

public abstract class Provider {
	String name;
	boolean cash, wallet, cc;
	public Provider(String name) {
		this.name = name;
		this.cash = true;
		this.wallet = true;
		this.cc = true;
	}
	public abstract boolean Pay();
	public String getName() { 
		return this.name;
	}
}
